    @Test
    public void addCluster() {

        // arrange
        Service service1 = new Service();
        Cluster cluster = new Cluster();
        cluster.setName("John Smith");

        // act
        service1.addCluster(cluster);

        // assert
        assertThat(service1.getClusterMap().get("John Smith"), sameInstance(cluster));
    }
